{
  "compilerOptions": {
    "baseUrl": ".",
    "esModuleInterop": true,
    "module": "commonjs",
    "jsx": "react",
    "lib": ["es2017", "dom", "esnext"],
    "sourceMap": true,
    "target": "es6",
    "noUnusedLocals": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "src"
  ],
  "exclude": ["node_modules"]
}
